    "def modulo_array(array):\n",
    "    #Calcular o módulo desse array\n",
    "    modulo = np.sqrt(np.sum(array**2))\n",
    "    return modulo\n",
    "#Exemplo de uso da função\n",
    "array = np.array([[1,2,3],[4,5,6]])\n",
    "modulo_array(array)"
   ]
