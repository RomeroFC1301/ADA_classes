    {
      "cell_type": "code",
      "source": [
        "def func_norm(array):\n",
        "  media = array.mean()\n",
        "  dvp = array.std()\n",
        "  return (array-media)/dvp\n",
        "rand = np.random.rand(20)\n",
        "resultado = func_norm(rand)\n",
        "print(resultado)"
